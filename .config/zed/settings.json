{
  "cursor_blink": false,
  "scrollbar": { "show": "never" },
  "scroll_beyond_last_line": "off",
  "vertical_scroll_margin": 0,
  "auto_indent_on_paste": true,
  "vim_mode": true,
  "ui_font_size": 20,
  "buffer_font_size": 20,
  "autosave": {
    "after_delay": {
      "milliseconds": 1000
    }
  },
  "indent_guides": {
    "enabled": true,
    "coloring": "indent_aware"
  },
  "command_aliases": {
    "W": "w",
    "Wq": "wq",
    "Q": "q"
  },
  "terminal": {
    "dock": "bottom"
  },
  "features": {
    "inline_completion_provider": "none"
  },
  "inlay_hints": {
    "enabled": true
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "project_panel": {
    "button": true,
    "dock": "left",
    "git_status": false
  },
  "notification_panel": {
    "button": false
  },
  "chat_panel": {
    "button": false
  },
  "collaboration_panel": {
    "button": false
  },
  "theme": {
    "mode": "system",
    "light": "Ayu Dark",
    "dark": "Ros√© Pine"
  }
}
